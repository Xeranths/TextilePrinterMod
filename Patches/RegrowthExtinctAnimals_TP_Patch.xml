<?xml version="1.0" encoding="utf-8" ?>
<Patch>
	<Operation Class="PatchOperationFindMod">
		<mods>
			<li>ReGrowth: Extinct Animals Pack</li>
		</mods>
		<match Class="PatchOperationSequence">
			<success>Normal</success>
			<operations>
				<li Class="PatchOperationAdd">
					<xpath>Defs</xpath>
					<value>
						<RecipeDef ParentName="CreateLeatherFromOrganic">
							<defName>Make_WoolyRhinocerosWoolFromOrganic</defName>
							<label>make wooly rhinoceros wool from organic</label>
							<description>Synthetize atoms from organic into wooly rhinoceros wool.</description>
							<products>
								<RG-EAP_WoollyRhinocerosWool>200</RG-EAP_WoollyRhinocerosWool>
							</products>
						</RecipeDef>
					</value>		
				</li>	
			</operations>
		</match>
	</Operation>
</Patch>