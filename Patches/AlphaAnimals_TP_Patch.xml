<?xml version="1.0" encoding="utf-8" ?>
<Patch>
	<Operation Class="PatchOperationFindMod">
		<mods>
			<li>Alpha Animals</li>
		</mods>
		<match Class="PatchOperationSequence">
			<success>Normal</success>
			<operations>
				<li Class="PatchOperationAdd">
				   <xpath>Defs</xpath>
				      <value>

					<!-- Recipes of gallatross -->

  					<RecipeDef ParentName="CreateTextile">
   					 <defName>Make_LeatherGallatrossFromOrganic</defName>
   					 <jobString>Synthetizing gallatross leather from organic.</jobString>
   					 <label>make gallatross leather from organic</label>
   					 <description>Synthetize atoms from organic into Gallatross leather. The reproduction of the leather of this mighty creature need a DNA sample</description>
   					 <ingredients>
   					   <li>
      					   <filter>
	  				   <categories>
					   <li>PlantMatter</li>
					   <li>Foods</li>
	  				   </categories>
	 				   <disallowedCategories>
          				      <li>FoodMeals</li>
        				   </disallowedCategories>
      					   </filter>
     					   <count>15</count>
     					   </li>
					   <li>
      					    <filter>
	    				    <thingDefs>
						<li>Meat_AA_Gallatross</li>
	    				    </thingDefs>
      					    </filter>
       					    <count>5</count>
   					    </li>
  					  </ingredients>
    					  <fixedIngredientFilter>
					    <categories>
	  				      <li>PlantMatter</li>
					      <li>Foods</li>
					    </categories>
					    <disallowedCategories>
        				       <li>FoodMeals</li>
      					    </disallowedCategories>
  					  </fixedIngredientFilter>
  					  <defaultIngredientFilter>
     					    <categories>
     					     <li>FoodRaw</li>
    					    </categories>
     					  <disallowedCategories>
      					    <li>AnimalProductRaw</li>
    					  </disallowedCategories>
   					  </defaultIngredientFilter>
   					  <products>
    					    <Leather_Gallatross>50</Leather_Gallatross>
   					  </products>
   					  <researchPrerequisite>TextilePrinterResearchAdv</researchPrerequisite>
  					</RecipeDef>

  				   </value>
				</li>
				
				<!--  insects chitins -->
				
				<li Class="PatchOperationAdd">
				   <xpath>Defs</xpath>
				    <value>
						<RecipeDef ParentName="CreateLeatherFromOrganic">
							<defName>Make_AerofleetLeatherFromOrganic</defName>
							<label>make aerofleet leather from organic</label>
							<description>Synthetize atoms from organic into Aerofleet leather.</description>
							<products>
								<AA_Leather_Aerofleet>75</AA_Leather_Aerofleet>
							</products>
						</RecipeDef>
					</value>
				</li>
				
				<li Class="PatchOperationAdd">
				   <xpath>Defs</xpath>
				    <value>
						<RecipeDef ParentName="CreateLeatherFromOrganic">
							<defName>Make_ArcticLionLeatherFromOrganic</defName>
							<label>make arctic lion leather from organic</label>
							<description>Synthetize atoms from organic into Arctic Lion leather.</description>
							<products>
								<AA_ArcticLion_Leather>75</AA_ArcticLion_Leather>
							</products>
						</RecipeDef>
					</value>
				</li>
				
				<li Class="PatchOperationAdd">
				   <xpath>Defs</xpath>
				    <value>
						<RecipeDef ParentName="CreateLeatherFromOrganic">
							<defName>Make_SandLionLeatherFromOrganic</defName>
							<label>make sand lion leather from organic</label>
							<description>Synthetize atoms from organic into Sand Lion leather.</description>
							<products>
								<AA_SandLion_Leather>75</AA_SandLion_Leather>
							</products>
						</RecipeDef>
					</value>
				</li>
				
				<li Class="PatchOperationAdd">
				   <xpath>Defs</xpath>
				    <value>
						<RecipeDef ParentName="CreateLeatherFromOrganic">
							<defName>Make_ChitinFromOrganic</defName>
							<label>make chitin from organic</label>
							<description>Synthetize atoms from organic into chitin. Consume more due to the structure of the chitin.</description>
							<products>
								<Leather_Chitin>45</Leather_Chitin>
							</products>
						</RecipeDef>
					</value>
				</li>
				
				<li Class="PatchOperationAdd">
				   <xpath>Defs</xpath>
				    <value>
						<RecipeDef ParentName="CreateTextile">
							<defName>Make_IronChitinFromOrganic</defName>
							<label>make iron chitin from organic and steel</label>
							<description>Synthetize atoms from organic into Iron Chitin. Consume a lot for a few products, and cost steel.</description>
							<jobString>Synthetizing leather\wool from organic.</jobString>
							<researchPrerequisite>TextilePrinterResearchAdv</researchPrerequisite>
							<ingredients>
								<li>
									<filter>
										<categories>
											<li>PlantMatter</li>
											<li>Foods</li>
										</categories>
										<disallowedCategories>
											<li>FoodMeals</li>
										</disallowedCategories>
									</filter>
									<count>20</count>
								</li>
								<li>
									<filter>
										<thingDefs>
											<li>Steel</li>
										</thingDefs>
									</filter>
									<count>10</count>
								</li>
								</ingredients>
								<fixedIngredientFilter>
									<categories>
										<li>PlantMatter</li>
										<li>Foods</li>
									</categories>
									<thingDefs>
										<li>Steel</li>
									</thingDefs>
									<disallowedCategories>
										<li>FoodMeals</li>
									</disallowedCategories>
								</fixedIngredientFilter>
								<defaultIngredientFilter>
									<categories>
										<li>FoodRaw</li>
									</categories>
									<thingDefs>
										<li>Steel</li>
									</thingDefs>
									<disallowedCategories>
										<li>AnimalProductRaw</li>
									</disallowedCategories>
								</defaultIngredientFilter>
							<products>
								<Leather_IronChitin>45</Leather_IronChitin>
							</products>
						</RecipeDef>
					</value>
				</li>
				
				<li Class="PatchOperationAdd">
				   <xpath>Defs</xpath>
				    <value>
						<RecipeDef ParentName="CreateLeatherFromOrganic">
							<defName>Make_RaptorPlatesFromOrganic</defName>
							<label>make raptor plates from organic</label>
							<description>Synthetize atoms from organic into somes raptor's plates. Consume a lot, for few plates.</description>
							<products>
								<Leather_RaptorPlates>25</Leather_RaptorPlates>
							</products>
						</RecipeDef>
					</value>
				</li>
				
				<li Class="PatchOperationAdd">
				   <xpath>Defs</xpath>
				    <value>
						<RecipeDef ParentName="CreateLeatherFromOrganic">
							<defName>Make_CactusLeatherFromOrganic</defName>
							<label>make cactus leather from organic</label>
							<description>Synthetize atoms from organic into cactus leather. The special structure of this plant-animal like isn't an obstacle to the printer, due to it's molecular reorganisation ability. However, it's not super cost efficient.</description>
							<products>
								<Leather_Cactus>60</Leather_Cactus>
							</products>
						</RecipeDef>
					</value>
				</li>
				
				<li Class="PatchOperationAdd">
				   <xpath>Defs</xpath>
				    <value>
						<RecipeDef ParentName="CreateLeatherFromOrganic">
							<defName>Make_BlackChitinFromOrganic</defName>
							<label>make black chitin from organic</label>
							<description>Synthetize atoms from organic into black chitin. Due to the chitin structure, it need a lot of materials for some products.</description>
							<products>
								<Leather_BlackChitin>40</Leather_BlackChitin>
							</products>
						</RecipeDef>
					</value>
				</li>
				
				<li Class="PatchOperationAdd">
				   <xpath>Defs</xpath>
				    <value>
						<RecipeDef ParentName="CreateLeatherFromOrganic">
							<defName>Make_NightLeatherFromOrganic</defName>
							<label>make night leather from organic</label>
							<description>Synthetize atoms from organic into night leather. A bit complex to reproduce, algorithms driving the printer are prone to waste a few resources.</description>
							<products>
								<Leather_Night>65</Leather_Night>
							</products>
						</RecipeDef>
					</value>
				</li>
				
				<li Class="PatchOperationAdd">
				   <xpath>Defs</xpath>
				    <value>
						<RecipeDef ParentName="CreateLeatherFromOrganic">
							<defName>Make_ChameleonYakWoolTemperateFromOrganic</defName>
							<label>make chameleon yak wool (temperate) from organic</label>
